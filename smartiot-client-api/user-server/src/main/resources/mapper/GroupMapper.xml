<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qzk.user.mapper.GroupMapper">

    <insert id="insertAndReturnId" useGeneratedKeys="true" keyProperty="id" parameterType="com.qzk.user.domain.entity.Group">
        INSERT INTO t_group (group_name,owner_id,create_time,update_time)
        values (#{groupName},#{ownerId},#{createTime},#{updateTime});
    </insert>
</mapper>
